{
  "name": "rerekisho-reaction-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "docker:start": "docker compose up -d",
    "dev": "tsx watch src/index.ts",
    "build": "npm run prisma:generate && tsc",
    "lint": "eslint .",
    "test": "dotenv -e .env.test -- jest --runInBand",
    "test:deploy": "dotenv -e .env.test -- npm run prisma:deploy",
    "test:run": "npm run test:deploy && npm run test",
    "test:watch": "npm run test:deploy && npm run test --watch",
    "start": "node dist/index.js",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:deploy": "prisma migrate deploy"
  },
  "overrides": {
    "lodash": "4.17.23"
  },
  "dependencies": {
    "@prisma/adapter-mariadb": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "argon2": "^0.44.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-mysql-session": "^3.0.3",
    "express-session": "^1.19.0",
    "ioredis": "^5.9.2",
    "mysql2": "^3.15.3",
    "node-cron": "^4.2.1",
    "prisma": "^7.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.6",
    "@types/express-mysql-session": "^3.0.8",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "jest": "^29.7.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
